<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.40.1">
  <bpmn:message id="Message_CustomerInquiry" name="Kundennachricht" />
  <bpmn:process id="aiCustomerService" name="Intelligenter Kundenservice mit AI-Agenten" isExecutable="true">
    <bpmn:startEvent id="StartEvent_Inquiry" name="Kundennachricht empfangen">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
      <bpmn:messageEventDefinition messageRef="Message_CustomerInquiry" />
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_Intent" name="AI Agent 1: Intent &#38; Sentiment erkennen" camunda:type="external" camunda:topic="ai-intent-detection">
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_0tf3qqc</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="ParallelGateway_Split">
      <bpmn:incoming>Flow_0tf3qqc</bpmn:incoming>
      <bpmn:outgoing>Flow_RAG</bpmn:outgoing>
      <bpmn:outgoing>Flow_Compensation</bpmn:outgoing>
      <bpmn:outgoing>Flow_0o6mpb8</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_GenerateResponse" name="AI Agent 2: Antwort generieren" camunda:type="external" camunda:topic="ai-generate-response">
      <bpmn:incoming>Flow_0o6mpb8</bpmn:incoming>
      <bpmn:outgoing>Flow_ToJoin</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RAG" name="AI Agent 3: Wissensdatenbank-Suche (RAG)" camunda:type="external" camunda:topic="ai-rag-search">
      <bpmn:incoming>Flow_RAG</bpmn:incoming>
      <bpmn:outgoing>Flow_ToJoin2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_Compensation" name="AI Agent 4: Entschädigung vorschlagen" camunda:type="external" camunda:topic="ai-compensation">
      <bpmn:incoming>Flow_Compensation</bpmn:incoming>
      <bpmn:outgoing>Flow_ToJoin3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="ParallelGateway_Join">
      <bpmn:incoming>Flow_ToJoin</bpmn:incoming>
      <bpmn:incoming>Flow_ToJoin2</bpmn:incoming>
      <bpmn:incoming>Flow_ToJoin3</bpmn:incoming>
      <bpmn:outgoing>Flow_0oqe8dg</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_QualityCheck" name="AI Agent 5: Qualitäts-Check der Antwort" camunda:type="external" camunda:topic="ai-quality-gate">
      <bpmn:incoming>Flow_0oqe8dg</bpmn:incoming>
      <bpmn:outgoing>Flow_Good</bpmn:outgoing>
      <bpmn:outgoing>Flow_Bad</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Quality">
      <bpmn:incoming>Flow_Good</bpmn:incoming>
      <bpmn:incoming>Flow_Bad</bpmn:incoming>
      <bpmn:outgoing>Flow_SendAuto</bpmn:outgoing>
      <bpmn:outgoing>Flow_Human</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_SendAuto" name="Automatisch an Kunde senden" camunda:type="external" camunda:topic="send-response">
      <bpmn:incoming>Flow_SendAuto</bpmn:incoming>
      <bpmn:outgoing>Flow_EndAuto</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_HumanReview" name="Menschlicher Agent prüft &#38; bearbeitet" camunda:assignee="support-team">
      <bpmn:incoming>Flow_Human</bpmn:incoming>
      <bpmn:outgoing>Flow_SendHuman</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_SendHuman" name="Antwort senden (nach Mensch)" camunda:type="external" camunda:topic="send-response">
      <bpmn:incoming>Flow_SendHuman</bpmn:incoming>
      <bpmn:outgoing>Flow_EndHuman</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Auto" name="Fall automatisch erledigt">
      <bpmn:incoming>Flow_EndAuto</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Human" name="Fall mit Mensch erledigt">
      <bpmn:incoming>Flow_EndHuman</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_Inquiry" targetRef="Task_Intent" />
    <bpmn:sequenceFlow id="Flow_RAG" sourceRef="ParallelGateway_Split" targetRef="Task_RAG" />
    <bpmn:sequenceFlow id="Flow_Compensation" sourceRef="ParallelGateway_Split" targetRef="Task_Compensation" />
    <bpmn:sequenceFlow id="Flow_ToJoin" sourceRef="Task_GenerateResponse" targetRef="ParallelGateway_Join" />
    <bpmn:sequenceFlow id="Flow_ToJoin2" sourceRef="Task_RAG" targetRef="ParallelGateway_Join" />
    <bpmn:sequenceFlow id="Flow_ToJoin3" sourceRef="Task_Compensation" targetRef="ParallelGateway_Join" />
    <bpmn:sequenceFlow id="Flow_Good" sourceRef="Task_QualityCheck" targetRef="Gateway_Quality" />
    <bpmn:sequenceFlow id="Flow_Bad" sourceRef="Task_QualityCheck" targetRef="Gateway_Quality" />
    <bpmn:sequenceFlow id="Flow_SendAuto" sourceRef="Gateway_Quality" targetRef="Task_SendAuto">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${qualityApproved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Human" sourceRef="Gateway_Quality" targetRef="Task_HumanReview">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${qualityApproved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_EndAuto" sourceRef="Task_SendAuto" targetRef="EndEvent_Auto" />
    <bpmn:sequenceFlow id="Flow_SendHuman" sourceRef="Task_HumanReview" targetRef="Task_SendHuman" />
    <bpmn:sequenceFlow id="Flow_EndHuman" sourceRef="Task_SendHuman" targetRef="EndEvent_Human" />
    <bpmn:sequenceFlow id="Flow_0o6mpb8" sourceRef="ParallelGateway_Split" targetRef="Task_GenerateResponse" />
    <bpmn:sequenceFlow id="Flow_0tf3qqc" sourceRef="Task_Intent" targetRef="ParallelGateway_Split" />
    <bpmn:sequenceFlow id="Flow_0oqe8dg" sourceRef="ParallelGateway_Join" targetRef="Task_QualityCheck" />
  </bpmn:process>

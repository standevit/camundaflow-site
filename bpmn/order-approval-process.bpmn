      <bpmn:incoming>Flow_1j58788</bpmn:incoming>
      <bpmn:outgoing>Flow_05</bpmn:outgoing>
      <bpmn:outgoing>Flow_18phx5h</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent" name="Fertig">

    <bpmn:sequenceFlow id="Flow_Yes" name="Ja" sourceRef="Gateway_Amount" targetRef="UserTask_GF">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${amount &gt; 5000}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_No" name="Nein" sourceRef="Gateway_Amount" targetRef="ServiceTask_Confirmation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${amount &lt;= 5000}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_04" sourceRef="UserTask_GF" targetRef="ServiceTask_Confirmation" />
    <bpmn:sequenceFlow id="Flow_05" sourceRef="ServiceTask_Confirmation" targetRef="EndEvent" />
    <bpmn:sequenceFlow id="Flow_04090ac" sourceRef="StartEvent" targetRef="UserTask_Fachbereich" />
    <bpmn:sequenceFlow id="Flow_0pw2vr7" sourceRef="UserTask_Fachbereich" targetRef="Gateway_Amount" />
    <bpmn:sequenceFlow id="Flow_117amuj" name="Ja" sourceRef="Gateway_Amount" targetRef="UserTask_GF">

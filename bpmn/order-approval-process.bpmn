      <bpmn:outgoing>Flow_04</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="ServiceTask_Confirmation" name="Bestätigung versenden" camunda:type="external" camunda:topic="send-confirmation">
